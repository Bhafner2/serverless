    "@angular-devkit/architect": "^0.1202.0",
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/language-service": "~12.2.0",
    "@angular/pwa": "~12.2.0",
    "@angularclass/hmr": "3.0.0",
    "@bison/global-auto-configurations": "^2.1.1",
    "@ngneat/transloco-keys-manager": "^2.7.2",
    "@ngneat/transloco-optimize": "^1.0.2",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/hmr-plugin": "^3.7.2",
    "@nrwl/angular": "^12.6.5",
    "@nrwl/jest": "^12.6.5",
    "@nrwl/workspace": "^12.6.5",
    "@types/jest": "^27.0.0",
    "@types/jsdom": "^16.2.13",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.17.9",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.13.7",
    "autoprefixer": "^9.8.6",
