    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "optionalDependencies": {
    "@nrwl/cypress": "^12.6.6",
    "cypress": "^7.7.0",
    "eslint-plugin-cypress": "^2.11.3",
    "local-web-server": "^3.0.7",
    "lws-redirect": "^1.0.5"
  },
  "jestSonar": {
    "reportPath": "jest-junit",
    "reportFile": "junit.xml",
    "indent": "4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
