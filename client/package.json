{
  "name": "futterplan",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "serve": "node ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js",
    "serve:allhosts": "npm run serve -- --host=0.0.0.0",
    "serve:mock": "npm run serve -- --configuration=mock",
    "build": "npm run openapi:build && npm run build:base -- --configuration=production && npm run transloco:optimize",
    "build:base": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build:stats": "npm run build:base -- --stats-json",
    "build:stats:prod": "npm run build:base -- --configuration=production --stats-json",
    "build:analyze": "npx webpack-bundle-analyzer dist/stats.json -p 8081",
    "i18n:extract": "transloco-keys-manager extract -l fr --sort -a",
    "i18n:find": "transloco-keys-manager find",
    "clean:dist": "rimraf ./dist",
    "clean:node_modules": "rimraf ./node_modules",
    "prebuild": "npm run clean:dist",
    "postinstall": "ngcc --create-ivy-entry-points --tsconfig ./tsconfig.app.json",
    "e2e": "node ./node_modules/@angular/cli/bin/ng e2e",
    "test": "node ./node_modules/@angular/cli/bin/ng test --watch=false",
    "test:inspect": "node --inspect-brk ./node_modules/@angular/cli/bin/ng test",
    "test:watch": "node ./node_modules/@angular/cli/bin/ng test --watch",
    "transloco:optimize": "transloco-optimize dist/assets/i18n",
    "lint": "npm-run-all --silent --parallel lint:eslint lint:stylelint openapi:lint",
    "lint:eslint": "nps eslint",
    "lint:prettier": "nps prettier",
    "lint:stylelint": "nps stylelint",
    "lint-staged": "lint-staged",
    "fix": "npm-run-all --silent --parallel fix:eslint fix:stylelint",
    "fix:eslint": "nps eslint:fix",
    "fix:prettier": "nps prettier:fix",
    "fix:stylelint": "nps stylelint:fix",
    "jest": "jest --no-cache --coverage --passWithNoTests",
    "jest:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "jest:sonar": "jest --no-cache --coverage --passWithNoTests --testResultsProcessor=jest-sonar-reporter",
    "jest:watch": "jest --watch",
    "cypress:open": "$(npm bin)/cypress open",
    "cypress:run": "$(npm bin)/cypress run",
    "serve:static": "npm-run-all --parallel serve:ssl serve:redirect",
    "serve:ssl": "npm run serve -- --ssl=true --port=443",
    "serve:ssl:dist": "ws --port 443 --spa index.html --directory dist --http2 --cors.origin --static.maxage 0",
    "serve:redirect": "ws --port 80 --stack redirect --redirect 'http -> https'",
    "openapi:lint": "openapi-generator-cli validate -i ../api/openapi.yaml",
    "openapi:build": "openapi-generator-cli generate -g typescript-angular -i ../api/openapi.yaml -c ./typescript-angular-generator.yaml -o ../client/src/app/gen"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "~12.2.0",
    "@angularclass/bootloader": "1.0.1",
    "@biskin-kit/ag-grid": "7.0.0-next.2",
    "@biskin-kit/core": "7.0.0-next.2",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngneat/transloco": "^2.21.0",
    "@ngneat/transloco-locale": "^1.4.0",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngxs-labs/select-snapshot": "^3.0.0",
    "@ngxs/logger-plugin": "^3.7.2",
    "@ngxs/router-plugin": "^3.7.2",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@smartfarming/common": "^1.5.0",
    "@smartfarming/icons": "^0.8.2",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "ag-grid-enterprise": "^25.3.0",
    "bootstrap": "^4.6.0",
    "css-vars": "^2.3.0",
    "date-fns": "^2.23.0",
    "guid-typescript": "^1.0.9",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "messageformat": "^2.3.0",
    "ng": "0.0.0",
    "ngx-toastr": "~14.1.3",
